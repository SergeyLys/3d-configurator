{
  "name": "webpack-boilerplate",
  "version": "4.0.0",
  "description": "Plain webpack 4 Boilerplate with Babel, SASS and lodash on board",
  "author": "Christoph von Gellhorn",
  "license": "MIT",
  "homepage": "https://github.com/cvgellhorn/webpack-boilerplate",
  "scripts": {
    "start": "./node_modules/webpack/bin/webpack.js -p --progress && node server.js",
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --progress --mode development --config webpack.config.dev.js",
    "build": "webpack -p --progress --mode production --config webpack.config.build.js",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://cvgellhorn@github.com/cvgellhorn/webpack-boilerplate.git"
  },
  "dependencies": {
    "@babel/core": "7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@tweenjs/tween.js": "^17.2.0",
    "axios": "^0.18.0",
    "babel-loader": "8.0.2",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-preset-env": "^2.0.0-alpha.20",
    "clean-webpack-plugin": "0.1.19",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "express": "^4.16.4",
    "fabric": "^2.4.1",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "http-server": "^0.11.1",
    "lodash": "4.17.11",
    "mobx": "^5.5.0",
    "node-sass": "4.9.3",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "three-full": "6.0.0",
    "url-loader": "1.1.1",
    "webpack": "4.19.1",
    "webpack-cli": "3.1.1",
    "webpack-dev-server": "3.1.8",
    "webpack-merge": "4.1.4"
  }
}
